    rdfs:subClassOf [ a owl:Restriction ;
            owl:maxCardinality 1 ;
            owl:onProperty prod_prof:type_value ],
        [ a owl:Restriction ;
            owl:allValuesFrom [ a rdfs:Datatype ;
                    owl:intersectionOf ( xsd:string [ a rdfs:Datatype ;
                                owl:onDatatype xsd:string ;
                                owl:withRestrictions ( [ xsd:pattern "^[0-9]{3}|[A-Z0-9]{2,10}$" ] ) ] ) ] ;
            owl:onProperty prod_prof:type_value ],
        [ a owl:Restriction ;
            owl:minCardinality 0 ;
            owl:onProperty prod_prof:type_value ],
        prod_prof:GeneralProductInformation ;
    skos:definition "Product type and classification" ;
    skos:exactMatch prod_prof:productType ;
    skos:inScheme prod_prof: ;
    skos:note "3-digit numeric code from GTIN-14 company prefix (first 3 digits after country code) or alphanumeric classification code (2-10 characters)",
        "Addresses product classification system requirement" ;
    prod_prof:sql_identifier "product_type" .

