prod_prof:ProductImages a owl:Class ;
    rdfs:label "ProductImages" ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:minCardinality 0 ;
            owl:onProperty prod_prof:images_value ],
        [ a owl:Restriction ;
            owl:maxCardinality 1 ;
            owl:onProperty prod_prof:images_value ],
        [ a owl:Restriction ;
            owl:allValuesFrom [ a rdfs:Datatype ;
                    owl:intersectionOf ( xsd:string [ a rdfs:Datatype ;
                                owl:onDatatype xsd:string ;
                                owl:withRestrictions ( [ xsd:pattern "^https?://.*\\.(jpg|jpeg|png|gif|bmp|webp)(,\\s*https?://.*\\.(jpg|jpeg|png|gif|bmp|webp))*$" ] ) ] ) ] ;
            owl:onProperty prod_prof:images_value ],
        prod_prof:GeneralProductInformation ;
    skos:definition "Product images for branding and visual identification" ;
    skos:exactMatch prod_prof:productImages ;
    skos:inScheme prod_prof: ;
    skos:note "Addresses product branding and visual identification requirement",
        "Images stored as comma-separated list of image URLs (JPEG, PNG, GIF, BMP, WebP formats)" ;
    prod_prof:sql_identifier "product_images" .

